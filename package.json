{
  "name": "geocoding-scenario",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "postinstall": "npm run generate-typescript-geocoding && npm run generate-typescript-data",
    "generate-typescript-geocoding": "rimraf -rf src/clients-geocoding-api && git clone https://github.com/PTV-Group/clients-geocoding-api src/clients-geocoding-api",
    "generate-typescript-data": "rimraf -rf src/clients-data-api && git clone https://github.com/PTV-Group/clients-data-api src/clients-data-api"
  },
  "dependencies": {
    "@deck.gl/react": "^9.0.19",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mapbox/polyline": "^1.2.1",
    "@mui/icons-material": "^5.16.1",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.16.1",
    "@ptvgroup/deployment-detector": "^2.5.1",
    "@turf/bbox": "^7.0.0",
    "@turf/boolean-point-in-polygon": "^7.0.0",
    "@turf/boolean-point-on-line": "^7.0.0",
    "@turf/helpers": "^7.0.0",
    "mapbox-gl": "^1.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.1.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.2",
    "@types/mapbox__polyline": "^1.0.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
